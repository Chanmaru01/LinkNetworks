<p id="notice"><%= notice %></p>
<h1>Tweets</h1>
    <% @tweets.each do |tweet| %>
        <%= tweet.content %>
        <%= tweet.user %>
        <%= link_to '投稿', tweet %>
        <%= link_to '編集', edit_tweet_path(tweet) %>
        <%= link_to '削除', tweet, method: :delete %>
    <% end %>
<%= render @tweets %>
<%= link_to '新規投稿', new_tweet_path %>
